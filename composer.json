{
    "name": "Gustek/FeatureBundle",
    "type": "library",
    "description": "This library provides easy way to manage feature toggles",
    "keywords": ["feature"],
    "homepage": "http://github.com/Guzol/GustekFeatureBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrzej Gustyn",
            "email": "gustynandrzej@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.0",
        "symfony/finder": "2.5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "autoload": {
        "psr-4": { "Gustek\\FeatureBundle\\": "" }
    },
    "target-dir": "Gustek/FeatureBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
